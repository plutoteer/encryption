","_normalizeClass","active","autoRefresh","_hoisted_6","trainingStatus","_hoisted_7","_hoisted_8","_hoisted_9","_hoisted_10","getStatusClass","status","getStatusText","_hoisted_11","_hoisted_12","totalEpochs","lastEpoch","_hoisted_13","_hoisted_14","epoch","loss","toFixed","accuracy","trainingData","length","chartError","_hoisted_15","_hoisted_16","_hoisted_17","_hoisted_18","retryChart","_hoisted_19","_hoisted_20","_hoisted_21","_Fragment","_renderList","item","key","learningRate","epochTime"],"sources":["E:\\Â§ß‰∏â‰∏ãÂ≠¶‰π†ËµÑÊñô\\È°πÁõÆÂÆûËÆ≠\\MPHEDevCombine\\MPHEDevFrontEnd\\ParticipantFrontEnd\\encryption\\src\\views\\TrainingChart.vue"],"sourcesContent":["<template>\n  <div class=\"training-chart-container\">\n    <!-- ËÉåÊôØË£ÖÈ•∞ -->\n    <div class=\"background-decoration\"></div>\n    \n    <div class=\"content-wrapper\">\n      <div class=\"chart-header\">\n        <h2>ËÆ≠ÁªÉËøõÂ∫¶ÁõëÊéß</h2>\n        <div class=\"chart-controls\">\n          <button @click=\"refreshData\" :disabled=\"loading\" class=\"refresh-btn\">\n            <span class=\"btn-icon\">üîÑ</span>\n            {{ loading ? 'Âä†ËΩΩ‰∏≠...' : 'Âà∑Êñ∞Êï∞ÊçÆ' }}\n          </button>\n          <button @click=\"toggleAutoRefresh\" class=\"auto-refresh-btn\" :class=\"{ active: autoRefresh }\">\n            <span class=\"btn-icon\">{{ autoRefresh ? '‚èπÔ∏è' : '‚ñ∂Ô∏è' }}</span>\n            {{ autoRefresh ? 'ÂÅúÊ≠¢Ëá™Âä®Âà∑Êñ∞' : 'ÂºÄÂßãËá™Âä®Âà∑Êñ∞' }}\n          </button>\n        </div>\n      </div>\n\n      <div class=\"status-info\" v-if=\"trainingStatus\">\n        <div class=\"status-card\">\n          <div class=\"card-header\">\n            <h3>üìä ËÆ≠ÁªÉÁä∂ÊÄÅ</h3>\n          </div>\n          <div class=\"card-content\">\n            <div class=\"status-item\">\n              <span class=\"status-label\">Áä∂ÊÄÅ:</span>\n              <span class=\"status-value\" :class=\"getStatusClass(trainingStatus.status)\">\n                {{ getStatusText(trainingStatus.status) }}\n              </span>\n            </div>\n            <div class=\"status-item\">\n              <span class=\"status-label\">ÊÄªËΩÆÊï∞:</span>\n              <span class=\"status-value\">{{ trainingStatus.totalEpochs }}</span>\n            </div>\n            <div class=\"status-item\" v-if=\"trainingStatus.lastEpoch\">\n              <span class=\"status-label\">ÊúÄÊñ∞ËΩÆÊ¨°:</span>\n              <span class=\"status-value\">\n                {{ trainingStatus.lastEpoch.epoch }} \n                (ÊçüÂ§±: {{ trainingStatus.lastEpoch.loss.toFixed(4) }}, \n                ÂáÜÁ°ÆÁéá: {{ trainingStatus.lastEpoch.accuracy.toFixed(2) }}%)\n              </span>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Âè™ÊúâÂú®ÊúâÊï∞ÊçÆÊó∂ÊâçÊòæÁ§∫ÂõæË°®ÂÆπÂô® -->\n      <div class=\"chart-container\" v-if=\"trainingData.length > 0 && !chartError\">\n        <div class=\"chart-wrapper\">\n          <canvas ref=\"chartCanvas\"></canvas>\n        </div>\n      </div>\n\n      <!-- ÂõæË°®ÈîôËØØÊèêÁ§∫ -->\n      <div class=\"chart-error\" v-if=\"chartError\">\n        <div class=\"error-icon\">‚ö†Ô∏è</div>\n        <p>ÂõæË°®Ê∏≤ÊüìÂ§±Ë¥•: {{ chartError }}</p>\n        <button @click=\"retryChart\" class=\"retry-btn\">\n          <span class=\"btn-icon\">üîÑ</span>\n          ÈáçËØïÂõæË°®\n        </button>\n      </div>\n\n      <!-- Êó†Êï∞ÊçÆÊó∂ÊòæÁ§∫ÊèêÁ§∫ -->\n      <div class=\"no-data\" v-if=\"!loading && trainingData.length === 0\">\n        <div class=\"no-data-icon\">üìà</div>\n        <p>ÊöÇÊó†ËÆ≠ÁªÉÊï∞ÊçÆÔºåËØ∑Á≠âÂæÖËÆ≠ÁªÉÂºÄÂßãÊàñÊ£ÄÊü•ÂêéÁ´ØËøûÊé•</p>\n        <button @click=\"refreshData\" class=\"retry-btn\">\n          <span class=\"btn-icon\">üîÑ</span>\n          ÈáçËØï\n        </button>\n      </div>\n\n      <div class=\"data-table\" v-if=\"trainingData.length > 0\">\n        <div class=\"table-header\">\n          <h3>üìã ËÆ≠ÁªÉÊï∞ÊçÆËØ¶ÊÉÖ</h3>\n        </div>\n        <div class=\"table-container\">\n          <table>\n            <thead>\n              <tr>\n                <th>ËΩÆÊ¨°</th>\n                <th>ÊçüÂ§±ÂÄº</th>\n                <th>ÂáÜÁ°ÆÁéá (%)</th>\n                <th>Â≠¶‰π†Áéá</th>\n                <th>Áî®Êó∂</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"item in trainingData\" :key=\"item.epoch\">\n                <td>{{ item.epoch }}</td>\n                <td>{{ item.loss.toFixed(6) }}</td>\n                <td>{{ item.accuracy.toFixed(2) }}%</td>\n                <td>{{ item.learningRate.toFixed(6) }}</td>\n                <td>{{ item.epochTime }}</td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, onMounted, onUnmounted, nextTick } from 'vue'\nimport Chart from 'chart.js/auto'\nimport { getTrainingHistory, getTrainingStatus } from '../api/training.js'\n\nconst chartCanvas = ref(null)\nconst chart = ref(null)\nconst trainingData = ref([])\nconst trainingStatus = ref(null)\nconst loading = ref(false)\nconst autoRefresh = ref(false)\nconst autoRefreshInterval = ref(null)\nconst chartError = ref(null)\n\n// Ëé∑ÂèñËÆ≠ÁªÉÊï∞ÊçÆ\nconst fetchTrainingData = async () => {\n  try {\n    loading.value = true\n    chartError.value = null\n    const response = await getTrainingHistory()\n    \n    if (response.data.success) {\n      trainingData.value = response.data.data || []\n      console.log('Ëé∑ÂèñÂà∞ËÆ≠ÁªÉÊï∞ÊçÆ:', trainingData.value.length, 'Êù°')\n      \n      // Â¶ÇÊûúÊúâÊï∞ÊçÆÔºåÂ∞ùËØïÂàõÂª∫ÂõæË°®\n      if (trainingData.value.length > 0) {\n        await createChart()\n      }\n    }\n  } catch (error) {\n    console.error('Ëé∑ÂèñËÆ≠ÁªÉÊï∞ÊçÆÂ§±Ë¥•:', error)\n    trainingData.value = []\n  } finally {\n    loading.value = false\n  }\n}\n\n// Ëé∑ÂèñËÆ≠ÁªÉÁä∂ÊÄÅ\nconst fetchTrainingStatus = async () => {\n  try {\n    const response = await getTrainingStatus()\n    \n    if (response.data.success) {\n      trainingStatus.value = response.data\n    }\n  } catch (error) {\n    console.error('Ëé∑ÂèñËÆ≠ÁªÉÁä∂ÊÄÅÂ§±Ë¥•:', error)\n  }\n}\n\n// Âà∑Êñ∞Êï∞ÊçÆ\nconst refreshData = async () => {\n  await Promise.all([fetchTrainingData(), fetchTrainingStatus()])\n}\n\n// ÂàáÊç¢Ëá™Âä®Âà∑Êñ∞\nconst toggleAutoRefresh = () => {\n  autoRefresh.value = !autoRefresh.value\n  \n  if (autoRefresh.value) {\n    autoRefreshInterval.value = setInterval(refreshData, 5000) // ÊØè5ÁßíÂà∑Êñ∞‰∏ÄÊ¨°\n  } else {\n    if (autoRefreshInterval.value) {\n      clearInterval(autoRefreshInterval.value)\n      autoRefreshInterval.value = null\n    }\n  }\n}\n\n// Ëé∑ÂèñÁä∂ÊÄÅÊñáÊú¨\nconst getStatusText = (status) => {\n  const statusMap = {\n    'idle': 'Á©∫Èó≤',\n    'completed': 'Â∑≤ÂÆåÊàê',\n    'training': 'ËÆ≠ÁªÉ‰∏≠'\n  }\n  return statusMap[status] || status\n}\n\n// Ëé∑ÂèñÁä∂ÊÄÅÊ†∑ÂºèÁ±ª\nconst getStatusClass = (status) => {\n  const classMap = {\n    'idle': 'status-idle',\n    'completed': 'status-completed',\n    'training': 'status-training'\n  }\n  return classMap[status] || ''\n}\n\n// ÂÆâÂÖ®Âú∞ÈîÄÊØÅÂõæË°®\nconst destroyChart = () => {\n  if (chart.value) {\n    try {\n      chart.value.destroy()\n      console.log('ÂõæË°®Â∑≤ÈîÄÊØÅ')\n    } catch (error) {\n      console.warn('ÈîÄÊØÅÂõæË°®Êó∂Âá∫Èîô:', error)\n    }\n    chart.value = null\n  }\n}\n\n// ÈáçËØïÂõæË°®\nconst retryChart = async () => {\n  chartError.value = null\n  if (trainingData.value.length > 0) {\n    await createChart()\n  }\n}\n\n// ÂàõÂª∫ÂõæË°®\nconst createChart = async () => {\n  try {\n    // Á≠âÂæÖDOMÊõ¥Êñ∞\n    await nextTick()\n    \n    // Ê£ÄÊü•canvasÂÖÉÁ¥†\n    if (!chartCanvas.value) {\n      throw new Error('CanvasÂÖÉÁ¥†‰∏çÂ≠òÂú®')\n    }\n    \n    // ÈîÄÊØÅÊóßÂõæË°®\n    destroyChart()\n    \n    // È™åËØÅÊï∞ÊçÆ\n    const validData = trainingData.value.filter(item => \n      item && \n      typeof item.epoch === 'number' && \n      typeof item.loss === 'number' && \n      typeof item.accuracy === 'number' &&\n      !isNaN(item.loss) && \n      !isNaN(item.accuracy)\n    )\n    \n    if (validData.length === 0) {\n      throw new Error('Ê≤°ÊúâÊúâÊïàÁöÑËÆ≠ÁªÉÊï∞ÊçÆ')\n    }\n    \n    console.log('ÂáÜÂ§áÂàõÂª∫ÂõæË°®ÔºåÊúâÊïàÊï∞ÊçÆ:', validData.length, 'Êù°')\n    \n    // ÂáÜÂ§áÊï∞ÊçÆ\n    const epochs = validData.map(item => item.epoch)\n    const losses = validData.map(item => item.loss)\n    const accuracies = validData.map(item => item.accuracy)\n    \n    // Ëé∑Âèñcanvas‰∏ä‰∏ãÊñá\n    const ctx = chartCanvas.value.getContext('2d')\n    if (!ctx) {\n      throw new Error('Êó†Ê≥ïËé∑ÂèñCanvas‰∏ä‰∏ãÊñá')\n    }\n    \n    // ÂàõÂª∫ÂõæË°®ÈÖçÁΩÆ\n    const chartConfig = {\n      type: 'line',\n      data: {\n        labels: epochs,\n        datasets: [\n          {\n            label: 'Âπ≥ÂùáÊçüÂ§±',\n            data: losses,\n            borderColor: 'rgb(255, 99, 132)',\n            backgroundColor: 'rgba(255, 99, 132, 0.1)',\n            tension: 0.1,\n            yAxisID: 'y',\n            fill: false,\n            borderWidth: 3,\n            pointRadius: 6,\n            pointHoverRadius: 8\n          },\n          {\n            label: 'Âπ≥ÂùáÂáÜÁ°ÆÁéá (%)',\n            data: accuracies,\n            borderColor: 'rgb(54, 162, 235)',\n            backgroundColor: 'rgba(54, 162, 235, 0.1)',\n            tension: 0.1,\n            yAxisID: 'y1',\n            fill: false,\n            borderWidth: 3,\n            pointRadius: 6,\n            pointHoverRadius: 8\n          }\n        ]\n      },\n      options: {\n        responsive: true,\n        maintainAspectRatio: false,\n        interaction: {\n          mode: 'index',\n          intersect: false,\n        },\n        plugins: {\n          title: {\n            display: true,\n            text: 'ËÆ≠ÁªÉËøõÂ∫¶ÁõëÊéß - ÊçüÂ§±ÂÄº‰∏éÂáÜÁ°ÆÁéáÂèòÂåñ',\n            font: {\n              size: 18,\n              weight: 'bold'\n            },\n            color: '#1f2937'\n          },\n          legend: {\n            display: true,\n            position: 'top',\n            labels: {\n              usePointStyle: true,\n              padding: 20,\n              font: {\n                size: 14\n              }\n            }\n          },\n          tooltip: {\n            backgroundColor: 'rgba(0, 0, 0, 0.8)',\n            titleColor: '#ffffff',\n            bodyColor: '#ffffff',\n            borderColor: '#ffffff',\n            borderWidth: 1,\n            cornerRadius: 8,\n            callbacks: {\n              label: function(context) {\n                const label = context.dataset.label || ''\n                const value = context.parsed.y\n                if (label.includes('ÂáÜÁ°ÆÁéá')) {\n                  return `${label}: ${value.toFixed(2)}%`\n                } else {\n                  return `${label}: ${value.toFixed(6)}`\n                }\n              }\n            }\n          }\n        },\n        scales: {\n          x: {\n            display: true,\n            title: {\n              display: true,\n              text: 'ËÆ≠ÁªÉËΩÆÊ¨°',\n              font: {\n                size: 14,\n                weight: 'bold'\n              }\n            },\n            grid: {\n              color: 'rgba(0, 0, 0, 0.1)'\n            }\n          },\n          y: {\n            type: 'linear',\n            display: true,\n            position: 'left',\n            title: {\n              display: true,\n              text: 'ÊçüÂ§±ÂÄº',\n              font: {\n                size: 14,\n                weight: 'bold'\n              }\n            },\n            min: 0,\n            grid: {\n              color: 'rgba(0, 0, 0, 0.1)'\n            }\n          },\n          y1: {\n            type: 'linear',\n            display: true,\n            position: 'right',\n            title: {\n              display: true,\n              text: 'ÂáÜÁ°ÆÁéá (%)',\n              font: {\n                size: 14,\n                weight: 'bold'\n              }\n            },\