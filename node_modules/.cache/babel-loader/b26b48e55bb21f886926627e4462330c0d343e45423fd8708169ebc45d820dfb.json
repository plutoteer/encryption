{"ast":null,"code":"import { ref, onMounted, computed } from 'vue';\nimport { getCoordinatorStatus, getCurrentParticipantInfo } from '../api/participant.js';\nexport default {\n  __name: 'OnlineStatus',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const loading = ref(false);\n    const statusData = ref(null);\n    const error = ref(null);\n\n    // Ëé∑ÂèñÂΩìÂâçÂèÇ‰∏éÊñπ‰ø°ÊÅØ\n    const currentParticipant = getCurrentParticipantInfo();\n\n    // ËÆ°ÁÆóÂ±ûÊÄß\n    const onlineCount = computed(() => statusData.value?.online_participants || 0);\n    const totalCount = computed(() => statusData.value?.expected_participants || 0);\n    const onlinePercentage = computed(() => {\n      if (totalCount.value === 0) return 0;\n      return Math.round(onlineCount.value / totalCount.value * 100);\n    });\n    const canProceed = computed(() => {\n      // Ê†πÊçÆÂú®Á∫øÂèÇ‰∏éÊñπÊï∞ÈáèÂà§Êñ≠ÊòØÂê¶ÂèØ‰ª•Âçè‰Ωú\n      return onlineCount.value >= 2; // Ëá≥Â∞ëÈúÄË¶Å2‰∏™ÂèÇ‰∏éÊñπÂú®Á∫ø\n    });\n    const participants = computed(() => statusData.value?.participants || []);\n    const onlineTimeout = computed(() => statusData.value?.online_timeout || 30);\n    const heartbeatInterval = computed(() => statusData.value?.heartbeat_interval || 5);\n    const minParticipants = computed(() => statusData.value?.min_participants || 2);\n\n    // Ëé∑ÂèñÂèÇ‰∏éÊñπÂêçÁß∞\n    const getParticipantName = id => {\n      const names = {\n        1: 'ÂèÇ‰∏éÊñπ1',\n        2: 'ÂèÇ‰∏éÊñπ2',\n        3: 'ÂèÇ‰∏éÊñπ3'\n      };\n      return names[id] || `ÂèÇ‰∏éÊñπ${id}`;\n    };\n\n    // Ëé∑ÂèñÂêéÁ´ØÁ´ØÂè£\n    const getBackendPort = url => {\n      if (!url) return 'Êú™Áü•';\n      const match = url.match(/:(\\d+)$/);\n      return match ? match[1] : 'Êú™Áü•';\n    };\n\n    // Ê†ºÂºèÂåñÊúÄÂêéÂøÉË∑≥Êó∂Èó¥\n    const formatLastHeartbeat = timestamp => {\n      if (!timestamp) return 'Êú™Áü•';\n      const lastHeartbeat = new Date(timestamp);\n      const now = new Date();\n      const diffMs = now - lastHeartbeat;\n      const diffSeconds = Math.floor(diffMs / 1000);\n      const diffMinutes = Math.floor(diffSeconds / 60);\n      if (diffSeconds < 60) {\n        return `${diffSeconds}ÁßíÂâç`;\n      } else if (diffMinutes < 60) {\n        return `${diffMinutes}ÂàÜÈíüÂâç`;\n      } else {\n        const diffHours = Math.floor(diffMinutes / 60);\n        return `${diffHours}Â∞èÊó∂Ââç`;\n      }\n    };\n\n    // Âà§Êñ≠ÊòØÂê¶‰∏∫ÂΩìÂâçÂèÇ‰∏éÊñπ\n    const isCurrentParticipant = id => {\n      return id === currentParticipant.id;\n    };\n\n    // Âà∑Êñ∞Áä∂ÊÄÅ\n    const refreshStatus = async () => {\n      loading.value = true;\n      error.value = null;\n      try {\n        const response = await getCoordinatorStatus();\n        statusData.value = response.data;\n      } catch (err) {\n        console.error('Ëé∑ÂèñÂú®Á∫øÁä∂ÊÄÅÂ§±Ë¥•:', err);\n        error.value = 'Êó†Ê≥ïËøûÊé•Âà∞ÂêéÁ´ØÊúçÂä°ÔºåËØ∑Ê£ÄÊü•ÂêéÁ´ØÊòØÂê¶Ê≠£Âú®ËøêË°å';\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    // ÁªÑ‰ª∂ÊåÇËΩΩÊó∂Ëé∑ÂèñÁä∂ÊÄÅ\n    onMounted(() => {\n      refreshStatus();\n\n      // ÊØè30ÁßíËá™Âä®Âà∑Êñ∞‰∏ÄÊ¨°\n      setInterval(() => {\n        if (!loading.value && !error.value) {\n          refreshStatus();\n        }\n      }, 30000);\n    });\n    const __returned__ = {\n      loading,\n      statusData,\n      error,\n      currentParticipant,\n      onlineCount,\n      totalCount,\n      onlinePercentage,\n      canProceed,\n      participants,\n      onlineTimeout,\n      heartbeatInterval,\n      minParticipants,\n      getParticipantName,\n      getBackendPort,\n      formatLastHeartbeat,\n      isCurrentParticipant,\n      refreshStatus,\n      ref,\n      onMounted,\n      computed,\n      get getCoordinatorStatus() {\n        return getCoordinatorStatus;\n      },\n      get getCurrentParticipantInfo() {\n        return getCurrentParticipantInfo;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","getCoordinatorStatus","getCurrentParticipantInfo","loading","statusData","error","currentParticipant","onlineCount","value","online_participants","totalCount","expected_participants","onlinePercentage","Math","round","canProceed","participants","onlineTimeout","online_timeout","heartbeatInterval","heartbeat_interval","minParticipants","min_participants","getParticipantName","id","names","getBackendPort","url","match","formatLastHeartbeat","timestamp","lastHeartbeat","Date","now","diffMs","diffSeconds","floor","diffMinutes","diffHours","isCurrentParticipant","refreshStatus","response","data","err","console","setInterval"],"sources":["E:/Â§ß‰∏â‰∏ãÂ≠¶‰π†ËµÑÊñô/È°πÁõÆÂÆûËÆ≠/MPHEDevCombine/MPHEDevFrontEnd/ParticipantFrontEnd/encryption/src/components/OnlineStatus.vue"],"sourcesContent":["<template>\r\n  <div class=\"online-status\">\r\n    <!-- Âú®Á∫øÁä∂ÊÄÅÊ¶ÇËßà -->\r\n    <div class=\"status-overview\">\r\n      <div class=\"overview-item\">\r\n        <div class=\"overview-icon\">üë•</div>\r\n        <div class=\"overview-content\">\r\n          <div class=\"overview-label\">Âú®Á∫øÂèÇ‰∏éÊñπ</div>\r\n          <div class=\"overview-value\">{{ onlineCount }}/{{ totalCount }}</div>\r\n        </div>\r\n      </div>\r\n      <div class=\"overview-item\">\r\n        <div class=\"overview-icon\">üìä</div>\r\n        <div class=\"overview-content\">\r\n          <div class=\"overview-label\">Âú®Á∫øÁéá</div>\r\n          <div class=\"overview-value\">{{ onlinePercentage }}%</div>\r\n        </div>\r\n      </div>\r\n      <div class=\"overview-item\">\r\n        <div class=\"overview-icon\">‚úÖ</div>\r\n        <div class=\"overview-content\">\r\n          <div class=\"overview-label\">ÂèØÂçè‰Ωú</div>\r\n          <div class=\"overview-value\">{{ canProceed ? 'ÊòØ' : 'Âê¶' }}</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ÈîôËØØÁä∂ÊÄÅÊòæÁ§∫ -->\r\n    <div v-if=\"error\" class=\"error-message\">\r\n      <div class=\"error-icon\">‚ö†Ô∏è</div>\r\n      <div class=\"error-content\">\r\n        <h4 class=\"error-title\">Êó†Ê≥ïËé∑ÂèñÂú®Á∫øÁä∂ÊÄÅ</h4>\r\n        <p class=\"error-text\">{{ error }}</p>\r\n        <button @click=\"refreshStatus\" class=\"retry-btn\">\r\n          <span class=\"retry-icon\">üîÑ</span>\r\n          <span class=\"retry-text\">ÈáçËØï</span>\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ÂèÇ‰∏éÊñπÁä∂ÊÄÅÂàóË°® -->\r\n    <div v-else class=\"participants-list\">\r\n      <div class=\"list-header\">\r\n        <h4 class=\"list-title\">ÂèÇ‰∏éÊñπÁä∂ÊÄÅËØ¶ÊÉÖ</h4>\r\n        <button @click=\"refreshStatus\" class=\"refresh-btn\" :disabled=\"loading\">\r\n          <span class=\"refresh-icon\">üîÑ</span>\r\n          <span class=\"refresh-text\">{{ loading ? 'Âà∑Êñ∞‰∏≠...' : 'Âà∑Êñ∞' }}</span>\r\n        </button>\r\n      </div>\r\n      \r\n      <div class=\"participants-grid\">\r\n        <div \r\n          v-for=\"participant in participants\" \r\n          :key=\"participant.id\"\r\n          class=\"participant-card\"\r\n          :class=\"{ \r\n            'current-participant': isCurrentParticipant(participant.id),\r\n            'online': participant.status === 'online',\r\n            'offline': participant.status === 'offline'\r\n          }\"\r\n        >\r\n          <div class=\"participant-header\">\r\n            <div class=\"participant-info\">\r\n              <div class=\"participant-name\">{{ getParticipantName(participant.id) }}</div>\r\n              <div class=\"participant-id\">ID: {{ participant.id }}</div>\r\n            </div>\r\n            <div class=\"status-indicator\">\r\n              <div class=\"status-dot\" :class=\"participant.status\"></div>\r\n              <span class=\"status-text\">{{ participant.status === 'online' ? 'Âú®Á∫ø' : 'Á¶ªÁ∫ø' }}</span>\r\n            </div>\r\n          </div>\r\n          \r\n          <div class=\"participant-details\">\r\n            <div class=\"detail-item\">\r\n              <span class=\"detail-label\">ÂêéÁ´ØÁ´ØÂè£:</span>\r\n              <span class=\"detail-value\">{{ getBackendPort(participant.url) }}</span>\r\n            </div>\r\n            <div class=\"detail-item\">\r\n              <span class=\"detail-label\">ÊúÄÂêéÂøÉË∑≥:</span>\r\n              <span class=\"detail-value\">{{ formatLastHeartbeat(participant.last_heartbeat) }}</span>\r\n            </div>\r\n          </div>\r\n          \r\n          <div v-if=\"isCurrentParticipant(participant.id)\" class=\"current-badge\">\r\n            <span class=\"current-text\">ÂΩìÂâçÂèÇ‰∏éÊñπ</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Á≥ªÁªü‰ø°ÊÅØ -->\r\n    <div v-if=\"!error\" class=\"system-info\">\r\n      <div class=\"info-item\">\r\n        <span class=\"info-label\">ÂøÉË∑≥Ë∂ÖÊó∂:</span>\r\n        <span class=\"info-value\">{{ onlineTimeout }}Áßí</span>\r\n      </div>\r\n      <div class=\"info-item\">\r\n        <span class=\"info-label\">ÂøÉË∑≥Èó¥Èöî:</span>\r\n        <span class=\"info-value\">{{ heartbeatInterval }}Áßí</span>\r\n      </div>\r\n      <div class=\"info-item\">\r\n        <span class=\"info-label\">ÊúÄÂ∞èÂèÇ‰∏éÊñπ:</span>\r\n        <span class=\"info-value\">{{ minParticipants }}‰∏™</span>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted, computed } from 'vue'\r\nimport { getCoordinatorStatus, getCurrentParticipantInfo } from '../api/participant.js'\r\n\r\nconst loading = ref(false)\r\nconst statusData = ref(null)\r\nconst error = ref(null)\r\n\r\n// Ëé∑ÂèñÂΩìÂâçÂèÇ‰∏éÊñπ‰ø°ÊÅØ\r\nconst currentParticipant = getCurrentParticipantInfo()\r\n\r\n// ËÆ°ÁÆóÂ±ûÊÄß\r\nconst onlineCount = computed(() => statusData.value?.online_participants || 0)\r\nconst totalCount = computed(() => statusData.value?.expected_participants || 0)\r\nconst onlinePercentage = computed(() => {\r\n  if (totalCount.value === 0) return 0\r\n  return Math.round((onlineCount.value / totalCount.value) * 100)\r\n})\r\nconst canProceed = computed(() => {\r\n  // Ê†πÊçÆÂú®Á∫øÂèÇ‰∏éÊñπÊï∞ÈáèÂà§Êñ≠ÊòØÂê¶ÂèØ‰ª•Âçè‰Ωú\r\n  return onlineCount.value >= 2 // Ëá≥Â∞ëÈúÄË¶Å2‰∏™ÂèÇ‰∏éÊñπÂú®Á∫ø\r\n})\r\nconst participants = computed(() => statusData.value?.participants || [])\r\nconst onlineTimeout = computed(() => statusData.value?.online_timeout || 30)\r\nconst heartbeatInterval = computed(() => statusData.value?.heartbeat_interval || 5)\r\nconst minParticipants = computed(() => statusData.value?.min_participants || 2)\r\n\r\n// Ëé∑ÂèñÂèÇ‰∏éÊñπÂêçÁß∞\r\nconst getParticipantName = (id) => {\r\n  const names = {\r\n    1: 'ÂèÇ‰∏éÊñπ1',\r\n    2: 'ÂèÇ‰∏éÊñπ2', \r\n    3: 'ÂèÇ‰∏éÊñπ3'\r\n  }\r\n  return names[id] || `ÂèÇ‰∏éÊñπ${id}`\r\n}\r\n\r\n// Ëé∑ÂèñÂêéÁ´ØÁ´ØÂè£\r\nconst getBackendPort = (url) => {\r\n  if (!url) return 'Êú™Áü•'\r\n  const match = url.match(/:(\\d+)$/)\r\n  return match ? match[1] : 'Êú™Áü•'\r\n}\r\n\r\n// Ê†ºÂºèÂåñÊúÄÂêéÂøÉË∑≥Êó∂Èó¥\r\nconst formatLastHeartbeat = (timestamp) => {\r\n  if (!timestamp) return 'Êú™Áü•'\r\n  \r\n  const lastHeartbeat = new Date(timestamp)\r\n  const now = new Date()\r\n  const diffMs = now - lastHeartbeat\r\n  const diffSeconds = Math.floor(diffMs / 1000)\r\n  const diffMinutes = Math.floor(diffSeconds / 60)\r\n  \r\n  if (diffSeconds < 60) {\r\n    return `${diffSeconds}ÁßíÂâç`\r\n  } else if (diffMinutes < 60) {\r\n    return `${diffMinutes}ÂàÜÈíüÂâç`\r\n  } else {\r\n    const diffHours = Math.floor(diffMinutes / 60)\r\n    return `${diffHours}Â∞èÊó∂Ââç`\r\n  }\r\n}\r\n\r\n// Âà§Êñ≠ÊòØÂê¶‰∏∫ÂΩìÂâçÂèÇ‰∏éÊñπ\r\nconst isCurrentParticipant = (id) => {\r\n  return id === currentParticipant.id\r\n}\r\n\r\n// Âà∑Êñ∞Áä∂ÊÄÅ\r\nconst refreshStatus = async () => {\r\n  loading.value = true\r\n  error.value = null\r\n  \r\n  try {\r\n    const response = await getCoordinatorStatus()\r\n    statusData.value = response.data\r\n  } catch (err) {\r\n    console.error('Ëé∑ÂèñÂú®Á∫øÁä∂ÊÄÅÂ§±Ë¥•:', err)\r\n    error.value = 'Êó†Ê≥ïËøûÊé•Âà∞ÂêéÁ´ØÊúçÂä°ÔºåËØ∑Ê£ÄÊü•ÂêéÁ´ØÊòØÂê¶Ê≠£Âú®ËøêË°å'\r\n  } finally {\r\n    loading.value = false\r\n  }\r\n}\r\n\r\n// ÁªÑ‰ª∂ÊåÇËΩΩÊó∂Ëé∑ÂèñÁä∂ÊÄÅ\r\nonMounted(() => {\r\n  refreshStatus()\r\n  \r\n  // ÊØè30ÁßíËá™Âä®Âà∑Êñ∞‰∏ÄÊ¨°\r\n  setInterval(() => {\r\n    if (!loading.value && !error.value) {\r\n      refreshStatus()\r\n    }\r\n  }, 30000)\r\n})\r\n</script>\r\n\r\n<style scoped>\r\n.online-status {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n}\r\n\r\n.status-overview {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));\r\n  gap: 15px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.overview-item {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 12px;\r\n  padding: 15px;\r\n  background: linear-gradient(135deg, rgba(102, 126, 234, 0.1) 0%, rgba(118, 75, 162, 0.1) 100%);\r\n  border: 1px solid rgba(102, 126, 234, 0.2);\r\n  border-radius: 15px;\r\n  backdrop-filter: blur(10px);\r\n}\r\n\r\n.overview-icon {\r\n  font-size: 1.5rem;\r\n  width: 40px;\r\n  height: 40px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\r\n  border-radius: 10px;\r\n  color: white;\r\n}\r\n\r\n.overview-content {\r\n  flex: 1;\r\n}\r\n\r\n.overview-label {\r\n  font-size: 0.85rem;\r\n  color: #64748b;\r\n  font-weight: 500;\r\n  margin-bottom: 2px;\r\n}\r\n\r\n.overview-value {\r\n  font-size: 1.2rem;\r\n  font-weight: 700;\r\n  color: #1e293b;\r\n}\r\n\r\n.error-message {\r\n  background: rgba(239, 68, 68, 0.1);\r\n  border: 1px solid rgba(239, 68, 68, 0.3);\r\n  border-radius: 15px;\r\n  padding: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 15px;\r\n}\r\n\r\n.error-icon {\r\n  font-size: 2rem;\r\n  color: #ef4444;\r\n}\r\n\r\n.error-content {\r\n  flex: 1;\r\n}\r\n\r\n.error-tit